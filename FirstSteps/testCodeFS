include stdio
include stdlib

 <int> lastUsedNumber = 0
 <int> facFive = 0

 <int>main(<char*> argv, <int*> argc)
{
calcFac(null)

	return 0
}

 <void>calcFac()
{
	 facFive = fac(5)

}

 <int>fac(<int> n)
{
	 lastUsedNumber = n
	(n is 1)?{return 0}
	(n is 0)?{return 1}
<int> result = n * fac(n - 1)
	
	return result
}

 Olaf {
	 <int> count
	
	cons Olaf()
	{
		count = 42
	}
	
	cons Olaf(<int> count)
	{
		count = count
	}
 }

end
